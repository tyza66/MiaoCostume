
.top {
	height: 25px;
	width: 100%;
	background-color: #FFFFFF;
}
.mianfei{
	width: 100%;
	height: 120rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.ss{
	width: 100%;
	height: 60rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.kuang{
	width: 80%;
	height: 95%;
	background: #C8C7CC;
	border-radius: 15rpx;
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
}
.kuang input{
	width: 85%;
	height: 100%;
}
.kuang image{
	width: 50rpx;
	height: 50rpx;
}
.mianfei se-image{
	width: 80%;
	height: 100%;
}
.one{
	float: left;
	height: 340rpx;
	width: 49%;
	margin-left: 1%;
	margin-top: 10rpx;
	border-radius: 10rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	background: #F8F8F8;
}
.one .im{
	width: 100%;
	height: 60%;
}
.one .im image{
	width: 100%;
	height: 100%;
}
.one .ti{
	width: 100%;
	height: 20%;
}
.one .ti ._p{
	text-align: center;
	width: 100%;
	height: 20%;
}
.one .up{
	width: 100%;
	height: 20%;
	display: flex;
	flex-direction: row;
	justify-content: left;
	align-items: center;
}
.up image{
	width: 30rpx;
	height: 30rpx;
	margin-left: 10rpx;
}

